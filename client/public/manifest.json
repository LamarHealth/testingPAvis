{
  "short_name": "DocIt",
  "name": "DocIt",
  "version": "1.0.4",
  "description": "DocIt enables users to fill out web forms with OCR data",
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["./main.js"],
      "css": ["/main.css"]
    }
  ],
  "browser_action": {
    "default_title": "open the popup"
  },
  "manifest_version": 2,
  "background": {
    "scripts": ["background.js"]
  }
}

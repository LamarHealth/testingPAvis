{
    "short_name": "Speedify",
    "name": "Speedify.ai",
    "version": "2.0.0",
    "manifest_version": 2,
    "description": "Speedify gives your browser autofill everywhere. Upload any unstructured file (PDF, csv, etc.) and Speedify will automatically extract the data and fill it into any form.",
    "content_scripts": [
        {
            "matches": [
                "<all_urls>"
            ],
            "js": [
                "./main.js"
            ],
            "css": [
                "/main.css"
            ]
        }
    ],
    "browser_action": {
        "default_title": "open the popup"
    },
    "background": {
        "scripts": [
            "background.js"
        ]
    },
    "permissions": [
        "storage"
    ],
    "web_accessible_resources": [
        "docview.html"
    ]
}
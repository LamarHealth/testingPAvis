{
  "short_name": "DocIt",
  "name": "DocIt Development Build",
  "version": "1.0",
  "description": "This extension is for beta testing",
  "permissions": ["storage"],
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["./main.js"],
      "css": ["/main.css"]
    }
  ],
  "browser_action": {
    "default_popup": "index.html",
    "default_title": "open the popup"
  },
  "manifest_version": 2
}
